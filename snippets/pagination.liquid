<div class="pagination">
 {% if pagination.pages > 1 %}
  
 <nav role="navigation">
   {% if pagination.previous %}
     <a href="{{ pagination.previous.url }}" class="btn" > Previous</a>
   {% endif %}

   {% if pagination.next %}
     <a href="{{ pagination.next.url }}" class="btn"> Next </a>
   {% endif %}
 </nav>
   
 {% endif %}
</div>
